{
  "name": "Serve YOLOv5 2.0",
  "type": "app",
  "version": "2.0.0",
  "description": "Deploy YOLOv5 2.0 as REST API service",
  "categories": [
    "neural network",
    "images",
    "object detection",
    "serve"
  ],
  "need_gpu": false,
  "gpu": "preferred",
